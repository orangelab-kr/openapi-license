  "license": "MIT",
  "scripts": {
    "serve": "yarn serve:dev",
    "serve:dev": "cross-env NODE_ENV=dev npx serverless offline --stage dev",
    "serve:stage": "cross-env NODE_ENV=stage npx serverless offline --stage stage",
    "serve:prod": "cross-env NODE_ENV=prod npx serverless offline --stage prod",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "deploy": "yarn deploy:prod",
    "deploy:prod": "serverless deploy --stage prod",
    "deploy:stage": "serverless deploy --stage stage",
    "deploy:dev": "serverless deploy --stage dev"
  },
  "dependencies": {
    "@sentry/node": "^6.0.4",
    "@sentry/tracing": "^6.0.4",
    "cheerio": "^1.0.0-rc.5",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "got": "^11.8.1",
    "joi": "^17.3.0",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "openapi-internal-sdk": "^1.2.3",
    "serverless-http": "^2.7.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/eslint": "^7.2.5",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/express": "^4.17.11",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.9",
    "@types/prettier": "^2.1.5",
    "@types/serverless": "^1.78.20",
    "@types/webpack-node-externals": "^2.5.0",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.2.0",
    "serverless": "^2.21.1",
    "serverless-dotenv-plugin": "^3.8.1",
    "serverless-offline": "^6.8.0",
    "serverless-webpack": "^5.4.0",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "webpack": "^5.19.0",
    "webpack-node-externals": "^2.5.2"
  }

{
  "name": "openapi-license",
  "description": "It performs the function of verifying the driver's license.",
  "version": "1.4.1",
  "main": "dist/index.js",
  "repository": "https://github.com/hikick/openapi-license.git",
  "author": "danieluhm2004 <daniel@hikick.kr>",
  "license": "MIT",
  "scripts": {
    "serve": "yarn serve:dev",
    "serve:dev": "cross-env NODE_ENV=dev npx serverless offline --stage dev",
    "serve:stage": "cross-env NODE_ENV=stage npx serverless offline --stage stage",
    "serve:prod": "cross-env NODE_ENV=prod npx serverless offline --stage prod",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "deploy": "yarn deploy:prod",
    "deploy:prod": "serverless deploy --stage prod",
    "deploy:stage": "serverless deploy --stage stage",
    "deploy:dev": "serverless deploy --stage dev"
  },
  "dependencies": {
    "@sentry/node": "^6.11.0",
    "@sentry/tracing": "^6.11.0",
    "@slack/webhook": "^6.0.0",
    "cheerio": "^1.0.0-rc.10",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "dayjs": "^1.10.6",
    "express": "^4.17.1",
    "got": "^11.8.1",
    "joi": "^17.4.2",
    "openapi-internal-sdk": "^1.6.2",
    "serverless-http": "^2.7.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/eslint": "^7.28.0",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/express": "^4.17.13",
    "@types/node": "^16.7.1",
    "@types/prettier": "^2.3.2",
    "@types/serverless": "^1.78.34",
    "@types/webpack-node-externals": "^2.5.2",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.1",
    "prettier": "^2.3.2",
    "serverless": "^2.55.0",
    "serverless-dotenv-plugin": "^3.8.1",
    "serverless-offline": "^8.0.0",
    "serverless-webpack": "^5.5.1",
    "ts-loader": "^9.2.5",
    "typescript": "^4.3.5",
    "webpack": "^5.51.1",
    "webpack-node-externals": "^3.0.0"
  }

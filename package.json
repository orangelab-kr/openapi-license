  "license": "MIT",
  "scripts": {
    "serve": "yarn serve:dev",
    "serve:dev": "cross-env NODE_ENV=dev npx serverless offline --stage dev",
    "serve:stage": "cross-env NODE_ENV=stage npx serverless offline --stage stage",
    "serve:prod": "cross-env NODE_ENV=prod npx serverless offline --stage prod",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "deploy": "yarn deploy:prod",
    "deploy:prod": "serverless deploy --stage prod",
    "deploy:stage": "serverless deploy --stage stage",
    "deploy:dev": "serverless deploy --stage dev"
  },
  "dependencies": {
    "@sentry/node": "^6.3.5",
    "@sentry/tracing": "^6.3.5",
    "cheerio": "^1.0.0-rc.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "dayjs": "^1.10.4",
    "express": "^4.17.1",
    "got": "^11.8.1",
    "joi": "^17.3.0",
    "morgan": "^1.10.0",
    "openapi-internal-sdk": "^1.5.3",
    "serverless-http": "^2.7.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/eslint": "^7.2.5",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/express": "^4.17.11",
    "@types/morgan": "^1.9.2",
    "@types/node": "^15.0.2",
    "@types/prettier": "^2.1.5",
    "@types/serverless": "^1.78.26",
    "@types/webpack-node-externals": "^2.5.0",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.2.0",
    "serverless": "^2.39.2",
    "serverless-dotenv-plugin": "^3.8.1",
    "serverless-offline": "^7.0.0",
    "serverless-webpack": "^5.4.2",
    "ts-loader": "^9.1.2",
    "typescript": "^4.1.3",
    "webpack": "^5.36.2",
    "webpack-node-externals": "^3.0.0"
  }
